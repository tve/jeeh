{
  "name": "TvE JNZv5",
  "url": "https://github.com/tve/",
  "vendor": "TvE",
  "build": {
    "cpu": "cortex-m0",
    "extra_flags": "-DSTM32L0 -DSTM32L082xx",
    "f_cpu": "32000000L",
    "mcu": "stm32l082kzt6"
  },
  "debug": {
    "debug_tool": "blackmagic",
    "default_tools": [ "blackmagic", "stlink" ],
    "openocd_target": "stm32l0",
    "svd_path": "STM32L053x.svd"
  },
  "frameworks": [
    "mbed",
    "stm32cube"
  ],
  "upload": {
    "maximum_ram_size": 20480,
    "maximum_size": 196608,
    "protocol": "blackmagic",
    "protocols": [
      "jlink",
      "stlink",
      "blackmagic",
      "mbed"
    ]
  }
}
