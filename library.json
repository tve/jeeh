{
    "name": "JeeH",
    "description": "Headers for framework- and platform-agnostic code",
    "version": "1.2",
    "keywords": "experimental",
    "repository": {
        "type": "git",
        "url": "https://github.com/jeelabs/jeeh.git"
    },
    "frameworks": [ "arduino", "cmsis", "libopencm3", "mbed", "stm32cube" ],
    "platforms": [ "atmelavr", "espressif32", "ststm32" ],
    "libArchive": false,
    "export": { "exclude": "*", "include": [ "jee*", "jee/*", "gfx/gfx*" ] },
    "build": {
        "srcFilter": [ "-<gfx/fontconvert/*>", "+<*.cpp>", "+<gfx/*.cpp>" ]
    }
}
